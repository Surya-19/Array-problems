l1=[1,23,45,56,67,89,100]
l2=[0,2,4,34,69]
l3=[]
i,j,k=0,0,0
while(i<len(l1) and j<len(l2)):
    if(l1[i]<l2[j]):
        l3.append(l1[i])
        k+=1;i+=1
    elif(l1[i]>l2[j]):
        l3.append(l2[j])
        k+=1;j+=1
while(i<len(l1)):
    l3.append(l1[i])
    i+=1
while(j<len(l2)):
    l3.append(l2[j])
    j+=1
print(l3)
